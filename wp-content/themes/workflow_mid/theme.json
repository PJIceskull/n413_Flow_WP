{
  "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "palette": [
        {
          "color": "#E5ECF1",
          "name": "Background",
          "slug": "background"
        },
        {
          "color": "#3498D8",
          "name": "Blue",
          "slug": "blue"
        },
        {
          "color": "#FA970A",
          "name": "Orange",
          "slug": "orange"
        },
        {
          "color": "#2ECC71",
          "name": "Green",
          "slug": "green"
        },
        {
          "color": "#FA1B0A",
          "name": "Red",
          "slug": "red"
        },
        {
          "color": "#9B59B6",
          "name": "Purple",
          "slug": "purple"
        },
        {
          "color": "#B5FA0A",
          "name": "Limegreen",
          "slug": "limegreen"
        }
      ]
    },
    "layout": {
      "contentSize": "1512px",
      "wideSize": "1512px"
    },
    "typography": {
      "fontFamilies": [
        {
          "name": "Primary",
          "slug": "Primary",
          "fontFamily": "'Roboto', sans-serif",
          "fontFace": [
            {
              "fontFamily": "Roboto",
              "fontWeight": "200, 800",
              "src": ["file:./assets/fonts/Roboto-VariableFont_wdth,wght.ttf"]
            }
          ]
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "Roboto"
    },
    "color": {
      "background": "var:preset|color|background"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontWeight": "700"
        },
        "spacing": {
          "margin": {
            "bottom": "0"
          }
        }
      },
      "h1": {
        "typography": {
          "fontSize": "32px"
        },
        "spacing": {
          "margin": {
            "top": "77px"
          }
        }
      }
    },
    "blocks": {
      "core/buttons": {
        "spacing": {
          "margin": { "top": "0" }
        }
      },
      "core/button": {
        "color": {
          "background": "#000000"
        }
      },
      "core/navigation": {
        "spacing": {
          "margin": {
            "top": "0"
          }
        }
      },
      "core/navigation-link": {
        "css": "text-decoration: none; list-style: none;"
      }
    }
  }
}
